# MindGen Environment Variables Example v0.1.0
# Copy this file to .env and fill in your actual API keys

# Flask Configuration
FLASK_SECRET_KEY=your-secret-key-here-change-this-in-production

# LLM API Keys
# Qwen (Alibaba Cloud) API Configuration
QWEN_API_KEY=your-qwen-api-key-here

# DeepSeek API Configuration
DEEPSEEK_API_KEY=your-deepseek-api-key-here

# Personal ChatGPT Server (Primary - for China users)
PERSONAL_CHATGPT_URL=your-personal-chatgpt-server-url-here
PERSONAL_CHATGPT_API_KEY=your-personal-chatgpt-api-key-here

# OpenAI GPT-4o API Configuration (Removed - using Personal ChatGPT Server only)
# GPT4O_API_KEY=your-openai-api-key-here

# Optional: Development Settings
# FLASK_DEBUG=true
# FLASK_HOST=0.0.0.0
# FLASK_PORT=5000

# Optional: Logging Configuration
# LOG_LEVEL=INFO
# LOG_FILE=logs/mindgen.log

# Optional: Performance Monitoring
# ENABLE_MONITORING=true
# MONITOR_INTERVAL=30

# Optional: System Configuration
# MAX_ITERATIONS=3
# QUALITY_THRESHOLD=0.8
# TIMEOUT_PER_ROUND=300

# Optional: Production Settings
# ENABLE_RATE_LIMITING=true
# MAX_REQUESTS_PER_MINUTE=60
# ENABLE_CORS=true
# CORS_ORIGINS=http://localhost:3000,https://yourdomain.com 